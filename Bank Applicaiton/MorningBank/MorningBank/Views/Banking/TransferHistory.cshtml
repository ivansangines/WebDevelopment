@model IEnumerable<MorningBank.Models.ViewsModel.TransactionHistoryModel>
@{
    ViewBag.Title = "TransferHistory";
}
<h2>Transfer History</h2>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TransactionDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CheckingAccountNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SavingAccountNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Amount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Transactionfee)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TransactionTypeName)
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TransactionDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CheckingAccountNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SavingAccountNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Amount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Transactionfee)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TransactionTypeName)
            </td>

        </tr>
    }
</table>
